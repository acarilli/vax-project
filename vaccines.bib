	keywords = {Adult, Aged, Female, Hand Disinfection, Health Surveys, Humans, Influenza Vaccines, Influenza, Human, Intention, Logistic Models, Male, Masks, Middle Aged, Pandemics, Social Class, Social Support, Socioeconomic Factors, Taiwan, Vaccination, Young Adult},
	pages = {e0122970},
